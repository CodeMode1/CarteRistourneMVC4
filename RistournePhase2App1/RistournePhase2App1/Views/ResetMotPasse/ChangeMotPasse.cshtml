@model RistournePhase2App1.Models.Supporteur

@{
    ViewBag.Titre = "Nouveau Mot Passe";
}

<h2>@ViewBag.Titre</h2>

<h3>Pour le Supporteur : @Model.Courriel</h3>

@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
    @Html.ValidationSummary(true)

    <fieldset>
        <legend>Supporteur</legend>

        @Html.HiddenFor(model => model.Id)

        <div class="editor-label">
            @Html.LabelFor(model => model.MotPasse)
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.MotPasse)
            @Html.ValidationMessageFor(model => model.MotPasse)
        </div>

        <p>
            <input type="submit" value="Sauvegarder" />
        </p>
    </fieldset>
}

<div>
    @Html.ActionLink("Retour Accueil", "Index", "Accueil")
</div>

@section Scripts {

}
